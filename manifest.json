{
  "manifest_version": 3,
  "name": "Auto Open Tabs",
  "version": "2.0",
  "description": "Save, manage, and open your favorite tabs instantly with ease.",
  "permissions": ["tabs", "storage", "contextMenus", "scripting", "notifications"],
  "action": {
    "default_title": "Manage URLs",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Open all saved tabs"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
